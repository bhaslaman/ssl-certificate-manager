        "name": "DER",
        "desc": "Binary encoded certificate. Used by Java platforms.",
        "ext": ".der, .cer"
      },
      "pfx": {
        "name": "PFX/P12",
        "desc": "Binary format that can contain certificate, private key, and chain. Used by Windows/IIS.",
        "ext": ".pfx, .p12"
      },
      "p7b": {
        "name": "P7B/PKCS#7",
        "desc": "Contains only certificates and chain, no private key. Used for CA bundles.",
        "ext": ".p7b, .p7c"
      },
      "jks": {
        "name": "JKS",
        "desc": "Java KeyStore format. Used by Java applications like Tomcat.",
        "ext": ".jks, .keystore"
      },
