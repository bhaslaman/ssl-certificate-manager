    "description": "Learn about SSL certificate formats, conversions, and API usage",
    "formats": {
      "title": "Certificate Formats",
      "pem": {
        "name": "PEM",
        "desc": "Base64 encoded certificate with -----BEGIN CERTIFICATE----- header. Most common format for Apache, Nginx.",
        "ext": ".pem, .crt, .cer, .key"
      },
      "der": {
        "name": "DER",
        "desc": "Binary encoded certificate. Used by Java platforms.",
        "ext": ".der, .cer"
      },
      "pfx": {
        "name": "PFX/P12",
        "desc": "Binary format that can contain certificate, private key, and chain. Used by Windows/IIS.",
        "ext": ".pfx, .p12"
      },
      "p7b": {
        "name": "P7B/PKCS#7",
        "desc": "Contains only certificates and chain, no private key. Used for CA bundles.",
        "ext": ".p7b, .p7c"
      },
      "jks": {
        "name": "JKS",
        "desc": "Java KeyStore format. Used by Java applications like Tomcat.",
        "ext": ".jks, .keystore"
      },
      "csr": {
        "name": "CSR",
        "desc": "Certificate Signing Request. Contains public key and subject info, sent to CA.",
        "ext": ".csr"
      }
    },
